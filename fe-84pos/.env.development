# Development Environment Variables cho FE-84POS
# File này sẽ được sử dụng khi chạy npm start

# API Base URL - Gateway service (localhost)
REACT_APP_API_URL=http://localhost:1106

# App Environment
REACT_APP_ENV=development

# Debug mode
REACT_APP_DEBUG=true

# Timeout settings
REACT_APP_TIMEOUT=30000

# Log level
REACT_APP_LOG_LEVEL=debug
